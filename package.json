  "license": "MIT",
  "dependencies": {
    "config": "^3.3.1",
    "express": "^4.17.3",
    "express-validator": "^6.5.0",
    "gsap": "file:gsap-bonus.tgz",
    "mongoose": "^5.9.18"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
